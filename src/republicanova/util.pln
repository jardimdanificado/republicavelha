local util:module = {}

typealias int = integer
typealias vector3 = { x:float,y:float,z:float}
typealias vector2 = { x:float,y:float,z:float}

function util.vector3(x:float,y:float,z:float):vector3
    return{x=x,y=y,z=z}
end

function util.Vector3Subtract(vec0:vector3,vec1:vector3):vector3
    return 
    {
        x = vec0.x - vec1.x,
        y = vec0.y - vec1.y,
        z = vec0.z - vec1.z
    }
end

return util